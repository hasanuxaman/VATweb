            VATTypeCal($('.trVATRate').val());

        });

        function VATTypeCal(VatRate) {

            var rate = parseFloat(VatRate.replace(/\,/g, ''));

            var type = "";

            if (rate >= 15) {
                type = "VAT";
            }
            else if (rate == 0) {
                type = "NonVAT";

            }
            else if (rate > 0 && rate < 15) {
                type = "OtherRate";
            }
            console.log(type);
            //$('.trType').val(type);
            $(".trType").select2("val", type);

            //console.log("VAT Type Set", Type)

        }

