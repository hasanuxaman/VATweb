            trkFixedTable("itemTable", 3, args);
        }
        function callingFixedTable_tracking() {
            var args = [150, 150, 150, 150, 150, 150, 150, 150, 100, 100, 100, 150, 150, 150, 100, 100, 150, 150, 100, 100, 150, 100, 100, 100, 100, 100, 100, 100, 100, 150, 200, 150, 150, 150, 150, 150, 150, 150, 150, 100];
            trkFixedTable("itemTable3", 3, args);
        }

        $(function () {
            callingFixedTable();
            callingFixedTable_tracking();
        })


