        function VATTypeCal(VatRate) {
            if (VatRate == undefined) {
                VatRate = 0;
            }
            var rate = parseFloat(VatRate);

            var type = "";

            if (rate >= 15) {
                type = "VAT";
            }
            else if (rate == 0) {
                type = "NonVAT";
            }
            else if (rate > 0 && rate < 15) {
                type = "OtherRate";
            }
            console.log(type);
            $(".trType").select2("val", type);
        };

