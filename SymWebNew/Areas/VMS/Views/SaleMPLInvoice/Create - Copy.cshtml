                            <div class="editor-label">
                                <label for="SD">SD Rate</label>
                            </div>
                            <div class="editor-field input-group">
                                @Html.TextBox("SD", null, new { @placeholder = "SD Rate", @class = " trSDRate NumberCheckAddDetail  form-control", })
