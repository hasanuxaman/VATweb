                                <th>Product Name</th>
                                <th>UOM</th>
                                <th>Quantity</th>
                                <th>NBRPrice</th>
                                <th>SubTotal</th>
                                <th>SDAmount</th>
                                <th>VATRate</th>
                                <th>VATAmount</th>
                                <th>SaleType</th>
                                <th>UOM Price</th>
                                <th>UOMc</th>
                                <th>UOMn</th>
                                <th>UOMQty</th>
                                <th>CurrencyValue</th>
                                <th>DollerValue</th>
                                <th>VATName</th>
                                <th>CConversionDate</th>
                            </tr>
                        </thead>

