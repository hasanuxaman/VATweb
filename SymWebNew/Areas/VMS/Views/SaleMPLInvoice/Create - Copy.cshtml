                        </div>

                        <div class="col-md-1">
                            <div class="editor-label">
                                <label for="Quantity">Quantity</label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("SaleQuantity", "0", new { @class = " cSaleQuantity priceLen right   form-control" })
