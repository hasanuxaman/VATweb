{
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model => model.Id)
    @Html.HiddenFor(model => model.Operation)

    <div class="headrow">
        @if (Model.Operation == "update")
        {
            <button type="button" title="Update Data" onclick="SaveData()" class="sym-btn-update">&nbsp;Update</button>
            <label class="pull-right"> Tanks > Edit</label>
        }
        else if (Model.Operation == "add")
        {
            <button type="button" title="Save Data" onclick="SaveData()" class="sym-btn-save">&nbsp;Save</button>
            <label class="pull-right"> Tanks > Create</label>
        }

        <button type="button" title="Go To Index" data-url="/VMS/TanksMPL?TransactionType=Other" onclick="GoTo(this)" class="sym-btn-detail">&nbsp;Index</button>
        <button type="button" title="Add New Issue" data-url="/VMS/TanksMPL/Create?tType=Other" onclick="GoTo(this)" id="" class="sym-btn-add ">&nbsp;New</button>
    </div>

    <div class="clearfix"></div>

    <div class="tab-content">
        <div id="Sale" class="tab-pane fade  in active">
            <div class="thinbox formDesign trkMY-0">
                <div class="th_thinbox">
                    <p>Basic</p>
                </div>

                <div class="row">

                    <div class="col-md-2">
                        <div class="editor-label">
                            <label for="TankCode">Tank Code</label>
                        </div>
                        <div class="editor-field">
                            @Html.TextBoxFor(model => model.TankCode, new { @placeholder = "Tank Code", @class = "cTankCode form-control required" })
                            @Html.ValidationMessageFor(model => model.TankCode)
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="editor-label">
                            <label for="ItemNo">Item Name</label>
                        </div>
                        <div class="editor-field" id="mydiv" data-myval="ProCode">
                            @Html.SimpleDropDown("ItemNo", "/Config/DropDown/Product?IsRaw=", new { @class = " trProduct  form-control selectDropdown" })
                        </div>
                    </div>

                    <div class="trk_5">
                        <div class="editor-label">
                            <label>&nbsp;</label>
                        </div>
                        <div class="editor-field">
                            <button type="button" title="Click to Search Product" id="productBtn" class="sym-btn-search"></button>
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="editor-label">
                            <label for="THeight"> THeight </label>
                        </div>
                        <div class="editor-field">
                            @Html.TextBoxFor(model => model.THeight, new { @placeholder = "THeight", @class = "cTHeight form-control" })
                            @Html.ValidationMessageFor(model => model.THeight)
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="editor-label">
                            <label for="Comments"> Comments </label>
                        </div>
                        <div class="editor-field">
                            @Html.TextAreaFor(model => model.Comments, new { @placeholder = "Comments", @class = "cComments form-control" })
                            @Html.ValidationMessageFor(model => model.Comments)
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="editor-label">
                            <label>Active Status</label>
                        </div>
                        <div class="editor-field" style="padding-left: 40px;">
                            @Html.CheckBox("IsActive", new { id = "IsActive", @class = "cIsActive form-control", @style = "width:20px;" })
                        </div>
                    </div>

                </div>
            </div>


            <div class="headrow">
                @if (Model.Operation == "update")
                {
                <button type="button" title="Update Data" onclick="SaveData()" class="sym-btn-update">&nbsp;Update</button>
                }
                else if (Model.Operation == "add")
