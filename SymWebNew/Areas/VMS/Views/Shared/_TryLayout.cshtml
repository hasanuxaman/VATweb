                    $(".ui-dialog-titlebar-close").hide();
                    $(this).load(url);
                },
                buttons: {
                    "Close": function () {
                        $(this).dialog("close");
                    }
                }
            });


            $(".dialog-report").dialog({
                title: 'Report',
                autoOpen: false,
