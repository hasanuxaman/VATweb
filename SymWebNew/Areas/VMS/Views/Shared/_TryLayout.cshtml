    <script>
        $(function () {
            sessionDateTime()
        })

        function sessionDateTime() {
            var date = $(".customDatePicker").val();
            var changeableDate = $(".customDatePickerChangeable").val();

            var today = FormatDate(new Date());
            if (date == "") {
                $(".customDatePicker").val(today);
            }

            if (changeableDate == "") {
                $(".customDatePickerChangeable").val(today);
            }
        }
