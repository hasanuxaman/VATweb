                               , { "sName": "ContactPersonName", "sWidth": "10%" }
                               , { "sName": "ContactPersonDesignation", "sWidth": "10%" }
                               , { "sName": "ContactPersonMobile", "sWidth": "10%" }
                               , { "sName": "ContactPersonEmail", "sWidth": "10%" }
                               , { "sName": "Website", "sWidth": "10%" }
                               , { "sName": "BlockStatus", "sWidth": "9%" }
                ]
            });
                        
            oTable.columnFilter({
                sPlaceHolder: "head:after",
                aoColumns: [{ type: "checkbox", "sClass": "chkAll" } 
                           , { type: "text" }                        
                           , { type: "text" }                        
                           , { type: "text" }                        
                           , { type: "text" }                        
                           , { type: "text" }                        
                           , { type: "text" }                        
                           , { type: "text" }                        
                           , { type: "text" }
                           , { type: "select", values:["Blocked","Not Blocked"] }
                ]
            });
        });
    </script>
