@model IEnumerable<SymViewModel.Common.JobCircularVM>
<div class="headrow">
        <a type="button" title="Add New Job Circular" href="/Common/JobCircular/Create" id="" class=" sym-btn-add ">&nbsp;New</a>
        <a type="button" title="Delete Job Circular" href="/Common/JobCircular/Delete" onclick="deletedData(this, 'myDataTable');" id="" class="sym-btn-remove">&nbsp;Delete</a>
 <label class="pull-right"> Job Circular</label>
</div>
<div class="thinbox">
    <div class="th_thinbox">
        <p>Job Circular List</p>
    </div>
    <p></p>
    <div class="row">
        <div id="container">
            <div id="demo">
                <table id="myDataTable" class="display">
                    <thead>
                      <tr>
                            <th class="center">
                                <input type="checkbox" class="chkAll" />Select All</th>
                            <th>JobTitle</th>
                            <th>Designation Name</th>
                            <th>Expriance</th>
                            <th>Deadline</th>
                            <th>Description</th>
                            <th>Active/Inactive</th>
                        </tr>
                        <tr>
                            <th class="center">Actions</th>
                            <th>JobTitle</th>
                            <th>Designation Name</th>
                            <th>Expriance</th>
                            <th>Deadline</th>
                            <th>Description</th>
                            <th>Active/Inactive</th>
                        </tr>
                        </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<div class="headrow">
        <button type="button" title="Add New Country" data-url="/Common/JobCircular/Create" id="" class=" sym-btn-add ">&nbsp;New</button>
        <button type="button" title="Delete Country" data-url="/Common/JobCircular/Delete" onclick="deletedData(this, 'myDataTable');" id="" class="sym-btn-remove">&nbsp;Delete</button>
</div>
<script>
    $(document).ready(function () {
        var oTable = $('#myDataTable').dataTable({
            "bServerSide": true,
            "sAjaxSource": "/Common/JobCircular/_index",
            "bProcessing": true,
            "aLengthMenu": tablelength(),
            "bJqueryUI": true,
            "bAutoWidth": false,
            "aoColumns": [
                            {
                                "sName": "Id",
                                "bSearchable": false,
                                "bSortable": false,
                                "sWidth": "9%",
                                "sClass":"center",
                                "fnRender": function (oObj) {
                                    return ' <input onclick="CheckAll(this)" type="checkbox" data-Id="' + oObj.aData[0] + '">&nbsp; <a title="Click on for Edit" style="text-decoration:none;" href=\"/Common/JobCircular/Edit/' + oObj.aData[0] + '\"  class=" sym-btn-edit-row"></a>' +
                                    '&nbsp; <a title="Click on for Delete" style="text-decoration:none;"  data-url="/Common/JobCircular/Delete" onclick="deletedData(this, myDataTable,' + oObj.aData[0] + ');"  class="sym-btn-remove-row"></a>';
                                }
                            }
                            , { "sName": "JobTitle", "sWidth": "30%" }
                            , { "sName": "DesignationName", "sWidth": "15%" }
                            , { "sName": "Expriance", "sWidth": "10%" }
                            , { "sName": "Deadline", "sWidth": "10%" }
                            , { "sName": "Description", "sWidth": "30%" }
                            , { "sName": "Active", "sWidth": "10%" }
            ]
        });
        oTable.columnFilter({
            sPlaceHolder: "head:after",
            aoColumns: [{ type: "checkbox", "sClass": "chkAll" }
                       , { type: "text" }
                       , { type: "text" }
                       , { type: "text" }
                       , { type: "text" }
                       , { type: "text" }
                       , { type: "text" }
                       , { type: "select", values: ["Active", "Inactive"] }
                       //, { type: "date-range" }
            ]
        });
        SelectAllForDelete();
    });
    </script>
